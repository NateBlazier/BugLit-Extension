{
    "manifest_version": 3,
    "name": "BugLit",
    "version": "1.0",
    "description": "Highlights links that match user-defined keywords.",
    "permissions": ["scripting", "activeTab", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
  
    "48": "BugLitBaby.png"
    
  }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }